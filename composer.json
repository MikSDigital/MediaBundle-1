{
    "name": "development-x/media-bundle",
    "description": "Symfony 3 Media Bundle Manager",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Zlatin Hristov",
            "email": "about@z-latko.info",
            "homepage": "https://z-latko.info"
        },
        {
            "name": "DevelopmentX Community",
            "homepage": "https://devx.biz"
        }
    ],
    "require": {
        "php": "^5.3 || ^7.0",
        "symfony/form": "~2.8|^3.0",
        "symfony/yaml": "~2.8|^3.0",
        "symfony/config": "~2.8|^3.0",
        "symfony/translation": "~2.8|^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8|~5.8|~6.0"
    },
    "autoload": {
        "psr-4": { 
            "MediaBundle\\": "src" 
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MediaBundle\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
